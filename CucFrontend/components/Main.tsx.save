rcimport React from "react";
import Attendance from "./Attendance";
import Timetable from "./Timetable";
import Nav from "./Nav";
import { createStackNavigator } from "@react-navigation/stack";
import { Constants } from 'expo-constants'

const Stack = createStackNavigator();
export default function main() {
  const screens = [
    { name: "Attendance", component: Attendance },
    { name: "Timetable", component: Timetable },
    { name: "Nav", component: Nav },
  ];
  return (
    <Stack.Navigator
      initialRouteName="Nav"
      screenOptions={{ headerShown: false, headerTitleContainerStyle: { paddingTop:  }}}
    >
      {screens.map((screen, idx) => (
        <Stack.Screen {...screen} key={idx} />
      ))}
    </Stack.Navigator>
  );
}
